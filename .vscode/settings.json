{
  /* General Settings */
  // Editor
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  /* Language Settings */
  // Typescript
  "typescript.validate.enable": false,
  "vue.doctor.status": true,
  "vue.codeActions.enabled": true,
  "[vue]": {
    "editor.tabSize": 2
  }
}
